  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:reset": "prisma db push --force-reset",
    "test:phase2": "jest --selectProjects unit-lib-phase2 integration-phase2",
    "test:phase3a": "jest --selectProjects unit-clients-phase3a unit-api-phase3a integration-phase3a",
    "test:phase3c": "jest --config jest.phase3c.config.js",
    "test:phase3c:unit": "jest --config jest.phase3c.config.js --selectProjects unit-lib-phase3c unit-api-phase3c",
    "test:phase3c:integration": "jest --config jest.phase3c.config.js --selectProjects integration-transactions-phase3c --runInBand",
    "test:phase3c:components": "jest --config jest.phase3c.config.js --selectProjects components-phase3c",
    "test:phase3c:performance": "jest --config jest.phase3c.config.js --selectProjects performance-phase3c --runInBand",
    "test:phase3c:e2e": "playwright test tests/e2e/*phase3c*",
    "test:phase3c:e2e:headed": "playwright test tests/e2e/*phase3c* --headed",
    "test:phase3c:e2e:debug": "playwright test tests/e2e/*phase3c* --debug",
    "test:phase3c:all": "npm run test:phase3c:unit && npm run test:phase3c:integration && npm run test:phase3c:components && npm run test:phase3c:e2e",
    "test:phase3d": "jest --config jest.config.phase3d.js",
    "test:phase3d:unit": "jest --config jest.config.phase3d.js --selectProjects unit-prices-phase3d",
    "test:phase3d:integration": "jest --config jest.config.phase3d.js --selectProjects integration-prices-phase3d --runInBand",
    "test:phase3d:all": "npm run test:phase3d:unit && npm run test:phase3d:integration",
    "test:phase3d:coverage": "jest --config jest.config.phase3d.js --coverage",
    "test:unit": "jest --selectProjects unit-lib-phase2 unit-clients-phase3a unit-api-phase3a",
    "test:integration": "jest --selectProjects integration-phase2 integration-phase3a --runInBand",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test": "npm run test:phase2"
  },
